{
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false
  },
  "code-runner.runInTerminal": true,
  "terminal.integrated.inheritEnv": false,
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "redhat.telemetry.enabled": false,
  "python.defaultInterpreterPath": "/opt/homebrew/Caskroom/miniforge/base/envs/pyenv/bin/python",
  "cmake.configureOnOpen": true,
  "C_Cpp.clang_format_fallbackStyle": "{BasenOnStyle: Google, IndentWidth: 4, ColumnLimit: 0}",
  "C_Cpp.default.cppStandard": "c++17",
  "[cpp]": {
    "editor.wordBasedSuggestions": "off",
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true,
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "code-runner.executorMap": {
    "cpp": "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
  },
  //R options
  "r.rpath.mac": "/opt/homebrew/Caskroom/miniforge/base/envs/renv/bin/R",
  "r.rterm.mac": "/opt/homebrew/Caskroom/miniforge/base/envs/renv/bin/radian",
  //
  "github.copilot.advanced": {},
  "json.schemas": [],
  "todo-tree.highlights.foregroundColourScheme": [
    "white",
    "white",
    "black",
    "pink"
  ],
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "todo-tree.highlights.customHighlight": {
    "checklist": {
      "background": "#20a904",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "check-circle-fill",
      "iconColour": "#20a904",
      "type": "text"
    },
    "note": {
      "background": "#56b0dd",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "star-fill",
      "iconColour": "#ffc404",
      "type": "text"
    },
    "todo": {
      "background": "#b782f9",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "pin",
      "iconColour": "#b782f9",
      "type": "text"
    },
    "TODO": {
      "background": "#b782f9",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "pin",
      "iconColour": "#b782f9",
      "type": "text"
    },
    "warning": {
      "background": "#ffc404",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "alert",
      "iconColour": "#ff9f04"
    },
    "[ ]": {
      "background": "#f87364",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "x",
      "iconColour": "#f87364",
      "type": "text"
    },
    "[x]": {
      "background": "#20a904",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "check",
      "iconColour": "#20a904",
      "type": "text"
    },
    "fixme": {
      "background": "#f87364",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "alert",
      "iconColour": "#f87364",
      "type": "text"
    },
    "comeback": {
      "background": "#91d0b3",
      "foreground": "#ffffff",
      "gutterIcon": true,
      "icon": "telescope",
      "iconColour": "#91d0b3",
      "type": "text"
    }
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "fixme",
    "todo",
    "XXX",
    "[ ]",
    "[x]",
    "comeback",
    "note",
    "checklist",
    "warning",
    "TODO"
  ],
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "todo-tree.filtering.scopes": [],
  "git.ignoreRebaseWarning": true,
  "editor.guides.bracketPairs": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "tabnine.experimentalAutoImports": true,
  "[liquid]": {
    "editor.defaultFormatter": "sissel.shopify-liquid"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.createEnvironment.trigger": "off",
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "workbench.colorTheme": "One Monokai",
  "github.copilot.preferredAccount": "racheliee",
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.largeFileOptimizations": false,
  "[sql]": {
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "workbench.settings.applyToAllProfiles": [],
  "projectManager.groupList": true,
  "projectManager.git.baseFolders": [
    "Users/rachelie/project",
    "Users/rachelie/uni"
  ],
  "projectManager.tags": ["Personal", "Work", "Uni"],
  "projectManager.git.maxDepthRecursion": 2,
  "projectManager.any.baseFolders": ["/Users/rachelie/project"],
  "projectManager.any.maxDepthRecursion": 1,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.ruff": "always",
    "source.fixAll.eslint": "always",
    "source.convertImportFormat": "always",
    "source.fixAll": "always",
    "source.addMissingImports": "always"
  },
  "[typst]": {
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
  "[typst-code]": {
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
}
